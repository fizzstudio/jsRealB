    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Personage/Personage.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/demos/Personage/Personage.js",
            "runtimeArgs": [
                "--no-deprecation"
            ]
        },
        {
            "type": "chrome",
            "name": "Personage/index.html",
            "request": "launch",
            "url": "http://0.0.0.0:8000/demos/Personage/index.html"
        },
        {
            "type": "chrome",
            "name": "Tests/testAll.html",
            "request": "launch",
            "url": "http://0.0.0.0:8000/Tests/testAll.html"
        }
    ]
}